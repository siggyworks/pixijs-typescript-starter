{
  "name": "pixijs-parcel-template",
  "version": "1.0.0",
  "description": "A typescript project template for PixiJs using ParcelJs",
  "scripts": {
    "test": "jest",
    "start": "parcel src/index.html -p 8000",
    "build": "parcel build src/index.html --out-dir dist"
  },
  "keywords": [
    "typescript",
    "pixijs"
  ],
  "author": "siggyworks",
  "license": "ISC",
  "devDependencies": {
    "@parcel/resolver-glob": "^2.0.0-nightly.2338",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-typescript-tsc": "^2.0.0-alpha.3",
    "@types/jest": "^27.0.1",
    "jest": "^27.2.0",
    "parcel": "^2.0.0-rc.0",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "pixi.js": "^6.1.3"
  },
  "parcelCleanPaths": [
    "dist"
  ],
  "staticFiles": {
    "staticPath": "assets",
    "watcherGlob": "**"
  }
}
